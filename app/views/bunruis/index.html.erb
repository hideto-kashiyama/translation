<p id="notice"><%= notice %></p>

<h1>Listing Bunruis</h1>

<table>
  <thead>
    <tr>
      <th>Bunrui</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bunruis.each do |bunrui| %>
      <tr>
        <td><%= bunrui.bunrui %></td>
        <td><%= link_to 'Show', bunrui %></td>
        <td><%= link_to 'Edit', edit_bunrui_path(bunrui) %></td>
        <td><%= link_to 'Destroy', bunrui, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bunrui', new_bunrui_path %>
